<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allen.schoolo2o.dao.ProductCategoryDao">

<select id="queryProductCategoryList" resultType="com.allen.schoolo2o.entity.ProductCaetegory" 
   parameterType="Long">
   SELECT 
    product_category_id,
    product_category_name,
    priority,
    create_time,
    shop_id
    From 
    tb_product_category
    WHERE
    shop_id=#{shopId}
</select>






</mapper>